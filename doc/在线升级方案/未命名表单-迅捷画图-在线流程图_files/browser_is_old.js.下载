; (function browserIsOld() {
    var userAgent = navigator.userAgent;
    var isOpera = userAgent.indexOf("Opera") > -1;
    var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 && !isOpera;
    var isEdge = userAgent.indexOf("Windows NT 6.1; Trident/7.0;") > -1 && !isIE;
    var isOldIe = false;
    if (isIE) {
        var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
        // reIE.test(userAgent);
        var fIEVersion = parseFloat(RegExp["$1"]);
        if (fIEVersion != 11 || fIEVersion != 10) {
            isOldIe = true;
        }
    }
    
    if (isEdge) {
        isOldIe = true;
    }
    if (isOldIe) {
        var browserSloganImg = '/images/browser_is_old/pic_liulanqii.png';  
        var broserArray = [{
            icon:'/images/browser_is_old/icon_Chrome.png',
            title:'chrome',
            name:'chrome',
            link:'https://www.google.cn/intl/zh-CN/chrome/'
        },
        {
            icon:'/images/browser_is_old/icon_qqie.png',
            title:'QQ浏览器',
            name:'qq',
            link:'https://browser.qq.com/'
        },
        {
            icon:'/images/browser_is_old/icon_IEliulanqi.png',
            title:'IE11',
            name:'IE11',
            link:'https://support.microsoft.com/zh-cn/help/17621/internet-explorer-downloads'
        },
        {
            icon:'/images/browser_is_old/icon_fox.png',
            title:'火狐浏览器',
            name:'firefox',
            link:'https://www.firefox.com.cn/'
        },
        {
            icon:'/images/browser_is_old/icon_sougouliulanqi.png',
            title:'搜狗浏览器',
            name:'搜狗',
            link:'https://ie.sogou.com/'
        }];
        var browserDownList = [{
            icon:'/images/browser_is_old/icon_liuctu.png',
            title:'迅捷流程图',
            down:'https://tj2.sjhfrj.com/software/94?s=1'
        },{
            icon:'/images/browser_is_old/icon_siweidt.png',
            title:'迅捷思维导图',
            down:'https://tj2.sjhfrj.com/software/95?s=1'
        }]
        var browserDownHtml = '';
        for(var i = 0; i<browserDownList.length; i++){
            var thisDown = browserDownList[i];
            browserDownHtml += '<li class="li'+ i + '"><img src="'+ thisDown['icon'] +'"/><div class="text"><p>' + thisDown['title'] +'</p><a href="' + thisDown['down'] + '">立即下载</a></div></li>'
        }
        var addCssRule = function addCssRule() {
            function createStyleSheet() {
                var head = document.head || document.getElementsByTagName('head')[0];
                var style = document.createElement('style');
                style.type = 'text/css';
                head.appendChild(style);
                return style.sheet || style.styleSheet;
            }
            var sheet = createStyleSheet();
            return function (selector, rules, index) {
                index = index || 0;
                if (sheet.insertRule) {
                    sheet.insertRule(selector + "{" + rules + "}", index);
                } else if (sheet.addRule) {
                    sheet.addRule(selector, rules, index);
                }
            }
        }();
        addCssRule('.browser-tips-wrapper', 'color:#222;text-align:center;background-color: #fff;width: 100%;height: 100%;left: 0px;top: 0px;position: fixed;text-align: center;z-index: 9999999;padding-top:78px;');
        addCssRule('.browser-tips-wrapper .browser-tips-slogan', 'text-align: center;*zoom: 1;position: relative;');
        addCssRule('.browser-tips-wrapper .browser-tips-slogan .slogan-title', 'height: 1em;line-height: 1;font-size: 20px;margin: 0 0 20px 0;*margin-bottom: 22px;margin-bottom: 22px\\9\\0;font-weight:bold;');
        addCssRule('.browser-tips-wrapper .slogan-description', 'color:#444;line-height: 1.5;font-size: 16px;margin: 0 0 15px 0;');
        addCssRule('.browser-tips-wrapper .browser-list', 'font-size: 0;text-align: center;');
        addCssRule('.browser-tips-wrapper .list-item', 'width:90px;text-decoration: none;color:#888;display: inline-block;*display: inline;*zoom: 1;vertical-align: middle;font-size: 14px;padding:10px 0;border-radius:4px;');
        addCssRule('.browser-tips-wrapper .list-item:hover', 'background:#FAFAFA;');
        addCssRule('.browser-tips-wrapper .list-item+.list-item', 'margin-left: 40px; *margin-left: 40px;margin-left: 40px\\9\\0;');
        addCssRule('.browser-tips-wrapper .list-item .icon', 'display: block;margin: 0 auto 15px;border:none;');
        addCssRule('.browser-down-list', 'padding:35px 0 50px;overflow:hidden;')
        addCssRule('.browser-down-list ul', 'height:70px;margin:0;')
        addCssRule('.browser-down-list ul li', 'display:inline-block;padding:0 40px; *zoom:1;*display: inline;')
        addCssRule('.browser-down-list ul .li0', 'border-right:1px solid #DEDEDE;')
        addCssRule('.browser-down-list ul img', 'display:block;width:70px;height:70px;float:left;')
        addCssRule('.browser-down-list ul .text', 'overflow:hidden;padding-left:20px;text-align:left;font-size:18px;')
        addCssRule('.browser-down-list ul .text a', 'margin-top:20px;display:block;height:28px;line-height:28px;border:1px solid #048EFD;color:#0090FF;border-radius:15px;width:90px;text-align:center;font-size:14px;')
        addCssRule('.browser-tips-wrapper','display:block;')
        addCssRule('.browser-down-list ul .text a:hover','background:#EBF6FF;')
        var browserListHtml = '';
        for (var browserIndex = 0; browserIndex < broserArray.length; browserIndex++) {
            var thisBrowser = broserArray[browserIndex];
            browserListHtml += '<a class="list-item" href="'+ thisBrowser['link'] +'" target="_blank"><img class="icon" src="' + thisBrowser['icon'] + '"><span class="title">' + thisBrowser['title'] + '</span></a>';
        }
        var browserIsOldHtml = '<div class="browser-tips-wrapper" style="z-index: 999999"><img src="' + browserSloganImg + '"></img><div class="browser-tips-slogan"><h1 class="slogan-title">很抱歉！您正在使用的浏览器版本过低，无法正常使用我们的网站</h1><p class="slogan-description">为了能够正常体验迅捷画图的功能，我们建议您下载客户端</p></div><div class="browser-down-list"><ul>'+ browserDownHtml +'</ul></div><p class="slogan-description">或尝试以下浏览器</p><div class="browser-list">' + browserListHtml + '</div></div>';
        document.write(browserIsOldHtml);
    }

})();
